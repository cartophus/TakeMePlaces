@{
    ViewBag.Title = "Home Page";
}

@section content{
    <div id="map" style="height:725px;"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 44.4268, lng: 26.1025},
          zoom: 14
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBivmkEYpHbv7hH9hnb1u-LBd-7g7rO-I4&callback=initMap"
            async defer></script>
}